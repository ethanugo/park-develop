<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.upto.park.dao.mapper.enter.EnterInfoMapper" >
    <resultMap id="BaseResultMap" type="com.upto.park.dao.model.enter.EnterInfo" >
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="enter_info_number" property="enterInfoNumber" jdbcType="VARCHAR" />
        <result column="usr_num" property="usrNum" jdbcType="VARCHAR" />
        <result column="buss_cd" property="bussCd" jdbcType="VARCHAR" />
        <result column="park_no" property="parkNo" jdbcType="VARCHAR" />
        <result column="park_name" property="parkName" jdbcType="VARCHAR" />
        <result column="ref_no" property="refNo" jdbcType="VARCHAR" />
        <result column="entry_time" property="entryTime" jdbcType="VARCHAR" />
        <result column="reserve" property="reserve" jdbcType="VARCHAR" />
        <result column="sign" property="sign" jdbcType="VARCHAR" />
        <result column="is_delete" property="isDelete" jdbcType="INTEGER" />
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
        <result column="modified_time" property="modifiedTime" jdbcType="TIMESTAMP" />
    </resultMap>

    <sql id="Base_Column_List" >
        id, enter_info_number, usr_num, buss_cd, park_no, park_name, ref_no, entry_time, reserve, sign, is_delete, create_time, modified_time
    </sql>

    <insert id="insertEnterInfo" parameterType="com.upto.park.dao.model.enter.EnterInfo">
        INSERT INTO
        park_enter_info
        (enter_info_number, usr_num, buss_cd, park_no, park_name, ref_no, entry_time, reserve, sign)
        VALUES
        (#{enterInfoNumber, jdbcType=VARCHAR}, #{usrNum, jdbcType=VARCHAR}, #{bussCd, jdbcType=VARCHAR},
        #{parkNo, jdbcType=VARCHAR}, #{parkName, jdbcType=VARCHAR}, #{refNo, jdbcType=VARCHAR},
        #{entryTime, jdbcType=VARCHAR}, #{reserve, jdbcType=VARCHAR}, #{sign, jdbcType=VARCHAR})
    </insert>

    <select id="checkNumberExit" resultType="java.lang.Integer">
        SELECT COUNT(1)
        FROM park_enter_info
        WHERE enter_info_number = #{enterInfoNumber}
        AND is_delete = 0
    </select>

    <select id="getEnterInfoList" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List"/>
        FROM park_enter_info
        WHERE is_delete = 0
    </select>

    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer" >
    </select>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer" >
    </delete>
</mapper>
